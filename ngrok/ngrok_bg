#!/bin/bash
#chkconfig:2345 70 30
#description:ngrok_bg
case "$1" in
	start)
		echo "start ngrok background"
		sh /opt/vps-ssr-ngrok/ngrok/start.sh
		..
		''
		*)
		exit 1
		..
		''
esac
